{
  "masterPicture": "favicon.png",
  "iconsPath": "/",
  "design": {
    "ios": {
      "pictureAspect": "backgroundAndMargin",
      "backgroundColor": "#f9f9f9",
      "margin": "14%",
      "assets": {
        "ios6AndPriorIcons": true,
        "ios7AndLaterIcons": true,
        "precomposedIcons": true,
        "declareOnlyDefaultIcon": false
      },
      "appName": "Lan Tian @ Blog"
    },
    "desktopBrowser": {
      "design": "raw"
    },
    "windows": {
      "pictureAspect": "noChange",
      "backgroundColor": "#03a9f4",
      "onConflict": "override",
      "assets": {
        "windows80Ie10Tile": true,
        "windows10Ie11EdgeTiles": {
          "small": true,
          "medium": true,
          "big": true,
          "rectangle": true
        }
      },
      "appName": "Lan Tian @ Blog"
    },
    "androidChrome": {
      "pictureAspect": "noChange",
      "themeColor": "#03a9f4",
      "manifest": {
        "name": "Lan Tian @ Blog",
        "startUrl": "https://lantian.pub",
        "display": "standalone",
        "orientation": "notSet",
        "onConflict": "override",
        "declared": true
      },
      "assets": {
        "legacyIcon": true,
        "lowResolutionIcons": true
      }
    },
    "safariPinnedTab": {
      "pictureAspect": "blackAndWhite",
      "threshold": 87.5,
      "themeColor": "#03a9f4"
    }
  },
  "settings": {
    "compression": 0,
    "scalingAlgorithm": "NearestNeighbor",
    "errorOnImageTooSmall": false,
    "readmeFile": false,
    "htmlCodeFile": true,
    "usePathAsIs": false
  }
}
