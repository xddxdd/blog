{
  "name": "blog-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "check": "astro check && eslint && npm run check-prettier",
    "check-prettier": "prettier --check --plugin=prettier-plugin-astro .",
    "build": "npm run build-thumbs && npm run build-astro && npm run build-webp",
    "build-astro": "rm -rf .astro && npm run check && astro build",
    "build-fast": "rm -rf .astro && npm run check && FAST_BUILD=1 astro build",
    "build-thumbs": "sh _scripts/thumbs.sh",
    "build-webp": "sh _scripts/webp.sh",
    "preview": "astro preview",
    "astro": "astro",
    "format": "eslint --fix && npm run format-prettier",
    "format-prettier": "prettier --write --plugin=prettier-plugin-astro .",
    "serve": "python3 -m http.server 8000 -d dist"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.3.7",
    "@astrojs/react": "^4.4.0",
    "@astrojs/sitemap": "^3.6.0",
    "@fontsource/fira-code": "^5.2.7",
    "@fontsource/ubuntu": "^5.2.8",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@hpcc-js/wasm-graphviz": "^1.13.0",
    "@hpcc-js/wasm-zstd": "^1.5.1",
    "@shikijs/rehype": "^3.13.0",
    "@types/mime-types": "^3.0.1",
    "@types/react": "^19.1.17",
    "@types/react-dom": "^19.1.9",
    "@waline/client": "^3.6.0",
    "astro": "^5.14.5",
    "astro-favicons": "^3.1.5",
    "autoprefixer": "^10.4.21",
    "bootstrap": "^5.3.8",
    "bootstrap.native": "^5.1.6",
    "cssnano": "^7.1.1",
    "cssnano-preset-advanced": "^7.0.9",
    "date-fns": "^4.1.0",
    "feed": "^5.1.0",
    "flag-icon-css": "^4.1.7",
    "glob": "^11.0.3",
    "instant.page": "^5.2.0",
    "mime-types": "^3.0.1",
    "plausible-tracker": "^0.3.9",
    "postcss": "^8.5.6",
    "postcss-inline-svg": "^6.0.0",
    "postcss-load-config": "^6.0.1",
    "postcss-preset-env": "^10.4.0",
    "postcss-sorting": "^9.1.0",
    "prism-themes": "^1.9.0",
    "react": "^19.0.0",
    "react-dom": "^19.1.1",
    "rehype-external-links": "^3.0.0",
    "rehype-katex": "^7.0.1",
    "rehype-parse": "^9.0.1",
    "rehype-picture": "^5.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark": "^15.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-inline-links": "^7.0.0",
    "remark-join-cjk-lines": "^1.0.13",
    "remark-math": "^6.0.0",
    "remark-mermaid": "^0.2.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "remark-stringify": "^11.0.0",
    "sass": "^1.93.2",
    "sass-material-colors": "0.0.5",
    "simple-lightbox": "^2.1.0",
    "ts-node": "^10.9.2",
    "typescript-esbuild": "^0.4.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/js-yaml": "^4.0.9",
    "@types/postcss-inline-svg": "^5.0.4",
    "@types/postcss-sorting": "^8.0.2",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "not dead",
    "Firefox 52",
    "IE >=8",
    "Chrome 49"
  ],
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid",
    "proseWrap": "always"
  }
}
